{
  "_id": "workspace-id",
  "name": "Classroom API",
  "description": "API collection for Classroom application",
  "requests": [
    {
      "_id": "register-request-id",
      "method": "POST",
      "url": "http://localhost:5000/api/users/register",
      "headers": [],
      "body": {
        "mimeType": "application/json",
        "text": "{\"name\":\"John Doe\",\"email\":\"john@example.com\",\"password\":\"password123\"}"
      },
      "name": "Register User",
      "description": "Create a new user",
      "auth": {
        "type": "none"
      }
    },
    {
      "_id": "login-request-id",
      "method": "POST",
      "url": "http://localhost:5000/api/users/login",
      "headers": [],
      "body": {
        "mimeType": "application/json",
        "text": "{\"email\":\"john@example.com\",\"password\":\"password123\"}"
      },
      "name": "Login User",
      "description": "Login and get JWT token",
      "auth": {
        "type": "none"
      }
    },
    {
      "_id": "profile-request-id",
      "method": "GET",
      "url": "http://localhost:5000/api/users/profile",
      "headers": [
        {
          "name": "Authorization",
          "value": "Bearer {{jwtToken}}",
          "description": "Bearer token from login response"
        }
      ],
      "name": "Get User Profile",
      "description": "Get the authenticated user's profile",
      "auth": {
        "type": "none"
      }
    }
  ]
}
